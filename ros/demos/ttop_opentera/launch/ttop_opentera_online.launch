<launch>
    <arg name="output" default="log"/>

    <include file="$(find t_top)/launch/opentera_teleop.launch">
        <arg name="is_stand_alone" value="false"/>
        <arg name="output" value="$(arg output)"/>
    </include>

    <node pkg="ttop_opentera" type="ttop_opentera_node" name="ttop_opentera_node" output="$(arg output)" launch-prefix="bash -c 'sleep 20; $0 $@' "/>
</launch>
